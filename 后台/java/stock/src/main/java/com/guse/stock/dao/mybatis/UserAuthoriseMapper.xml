<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guse.stock.dao.IUserAuthoriseDao">

	<select id="findByDevice" parameterType="java.lang.String" resultType="userAuthorise">
		select * from pl_user_authorise where equipment_number=#{deviceNumber}
	</select>
	
</mapper>