package org.treediagram.nina.network.exception;

/**
 * 编码解码器解码异常
 * 
 * @author kidal
 * 
 */
public class CoderDecodeException extends SocketException {
	private static final long serialVersionUID = 1328990021998708962L;

	public CoderDecodeException() {
		super();
	}

	public CoderDecodeException(String message, Throwable cause) {
		super(message, cause);
	}

	public CoderDecodeException(String message) {
		super(message);
	}

	public CoderDecodeException(Throwable cause) {
		super(cause);
	}
}
